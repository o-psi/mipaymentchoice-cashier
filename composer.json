{
    "name": "mipaymentchoice/cashier",
    "description": "Laravel Cashier integration for MiPaymentChoice payment gateway",
    "keywords": ["laravel", "cashier", "mipaymentchoice", "billing", "payment"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "you@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.2",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "MiPaymentChoice\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiPaymentChoice\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MiPaymentChoice\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
